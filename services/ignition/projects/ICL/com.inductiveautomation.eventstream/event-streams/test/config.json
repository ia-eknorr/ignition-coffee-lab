{
  "enabled": true,
  "source": {
    "type": "com.cirruslink.mqtt.engine.gateway.mqtt.source",
    "config": {
      "topic": "icl/#",
      "qos": 0
    }
  },
  "sourceEncoder": {
    "type": "ignition.string",
    "properties": {
      "encoding": "UTF-8"
    }
  },
  "handlers": [
    {
      "enabled": true,
      "type": "ignition.script",
      "config": {
        "userCode": "\tlogger \u003d system.util.getLogger(\"testLogger\")\n\tlogger.info(str(events[0]))"
      },
      "failureStrategy": {
        "type": "ABORT",
        "retryPolicy": null
      }
    }
  ],
  "batch": {
    "debounceMs": 100,
    "maxWaitMs": 1000,
    "maxQueueSize": 0,
    "overflow": "DROP_OLDEST"
  },
  "filter": {
    "enabled": false,
    "userCode": "\treturn True"
  },
  "transform": {
    "enabled": false,
    "userCode": "\treturn event.data"
  },
  "transformEncoder": {
    "type": "ignition.jsonObject",
    "properties": {
      "encoding": "UTF-8"
    }
  },
  "onError": {
    "enabled": false,
    "userCode": ""
  }
}